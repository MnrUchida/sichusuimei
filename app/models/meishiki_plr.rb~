class MeishikiPlr < ActiveRecord::Base
  attr_accessible :meishiki_id, :piller, :tenkan_id, :chishi_id

  def meishiki=(value)
    @meishiki = value
    @sekki = Sekki.find_by_year_and_month(_meishiki.year,_meishiki.month)
  end

  def beforeSekki?()

    if @meishikiData.getMeishikiDate() < @sekki.date then
       ret = true
    else
       ret = false
    end

    ret
  end
end
